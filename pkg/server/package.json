{
  "name": "server",
  "version": "1.0.0",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "start": "node -r ts-node/register -r dotenv/config ./src/index.ts",
    "db:migrate": "node -r dotenv/config ./node_modules/.bin/knex migrate:latest",
    "db:seed": "node -r dotenv/config ./node_modules/.bin/knex seed:run",
    "db:rollback": "node -r dotenv/config ./node_modules/.bin/knex migrate:rollback",
    "db:make": "node -r dotenv/config ./node_modules/.bin/knex migrate:make"
  },
  "dependencies": {
    "apollo-datasource": "^0.6.3",
    "apollo-server": "^2.9.12",
    "knex": "^0.20.2",
    "lodash": "^4.17.15",
    "pg": "^7.14.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/faker": "^4.1.8",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.149",
    "@types/uuid": "^3.4.6",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "faker": "^4.1.0",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  }
}
